{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "RPI_VERSION=2"
      ],
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "browse": {
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/include/peripherals"
        ]
      }
    }
  ],
  "version": 4
}